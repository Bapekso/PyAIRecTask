# === SECURITY ===
# Klucz używany przez backend do podpisywania tokenów JWT i szyfrowania danych.
SECRET_KEY=your_secret_key_here

# === API KEYS ===
# Klucze API do zewnętrznych usług (nie commituj prawdziwych wartości!)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
GROQ_API_KEY=your_groq_api_key

# === DATABASE CONFIGURATION ===
# Adres połączenia do bazy danych (PostgreSQL)
DATABASE_URL=postgres://postgres:yourpassword@db:5432/reqt_db

DB_DRIVER=postgresql
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=yourpassword
DB_NAME=reqt_db
DB_DATABASE=reqt_db

# === NETWORK CONFIGURATION ===
# Porty serwisów frontend i backend
FRONTEND_PORT=5173
BACKEND_PORT=8000


